<h1>フェス登録</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@fes) do |f| %>
      <%= render 'shared/error_messages' , object: f.object %>

      <%= f.label :fes_name  %>
      <%= f.text_field :fes_name %>

      <%= f.label :fes_place %>
      <%= f.text_field :fes_place %>

      <%= f.label :fes_data %>
      <%= f.text_field :fes_data %>

      <%= f.label :artist %>
      <%= f.text_field :artist %>

      <%= f.submit "Create festival", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
